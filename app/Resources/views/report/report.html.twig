{% extends 'base.html.twig' %}

{% block body %}
    <div id="content" style="padding-top: 50px;">
        <div class="row">
            <div class="col-lg-12 col-sm-12">
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                        {% for c in data.head %}
                            <th>{{ c }}</th>
                        {% endfor %}
                        </tr>
                    </thead>
                    <tbody>
                    {% for c in data.body %}
                        <tr>
                        {% for r in c %}
                            <td>{{ r }}</td>
                        {% endfor %}
                        </tr>
                    {% endfor %}
                    </tbody>
                    <tfoot>
                        <tr>
                        {% for c in data.foot %}
                             <td>{{ c }}</td>
                        {% endfor %}
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
{% endblock %}